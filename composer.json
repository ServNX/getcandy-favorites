{
    "name": "servnx/getcandy-favorites",
    "description": "Favorites Addon for GetCandy 2",
    "license": "MIT",
    "authors": [
        {
            "name": "Poppabear",
            "email": "servnx@gmail.com"
        }
    ],
    "require": {
        "getcandy/core": "^2.0-beta"
    },
    "require-dev": {
        "cartalyst/converter": "^6.1|^7.0",
        "kalnoy/nestedset": "^6.0",
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Servnx\\GetCandyFavorite\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Servnx\\GetCandyFavorite\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Servnx\\GetCandyFavorite\\GetCandyFavoritesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
